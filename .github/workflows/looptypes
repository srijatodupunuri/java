import java.util.*;
class loops
{
    public static void main(String[]args)
    {
       for(int i=0;i<10;i++)
       {
        System.out.println(i);
       }
       int i=0;
       while(i<14)
       {
        System.out.println(i);
        i++;
       }
       do
       {
        System.out.println(i);
        i++;
       }while(i<12);
       
       
    }
}
