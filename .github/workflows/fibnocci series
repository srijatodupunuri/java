import java.util.Scanner.*;
import java.util.*;
public class Arrayqts
{
  public static void main(String[]args)
  {
   int a=0;
   int b=1;
   int c;
   Scanner sc=new Scanner(System.in);
   System.out.println("enter number of terms:");
   int nums=sc.nextInt();
   for(int i=0;i<nums;i++)
   {
     c=a+b;
     System.out.println(c);
     a=b;
     b=c;
   }
  }
}
